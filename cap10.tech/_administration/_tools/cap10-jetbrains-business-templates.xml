<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="BusinessCommunicationTemplates">
    <!-- 1. Introduction Call Template -->
    <template name="cap10-partner-intro-call" value="Introduction Call with $PARTNER$ on $DATE$:
Agenda: $AGENDA$
Notes: $NOTES$
Next Steps: $NEXT$" description="Log an introductory call with a business partner" toReformat="true" toShortenFQNames="true">
        <variable name="DATE" expression="date()" defaultValue="YYYY-MM-DD" alwaysStopAt="false" />
        <variable name="PARTNER" expression="" defaultValue="Partner Name" alwaysStopAt="true" />
        <variable name="AGENDA" expression="" defaultValue="Discuss mutual interests and opportunities" alwaysStopAt="true" />
        <variable name="NOTES" expression="" defaultValue="Key discussion points" alwaysStopAt="true" />
        <variable name="NEXT" expression="" defaultValue="Follow-up actions" alwaysStopAt="true" />
        <context>
            <option name="TEXT" value="true" />
        </context>
    </template>

